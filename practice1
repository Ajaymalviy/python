class Deteil:
    def __init__(self):#constructor of this class
       #data member of this classs
       self.x=10
       self.b=20
       self.c=30
    def variable(self,f,g):#method of class
        self.d=30
        self.e=40
        self.f=f
        self.g=g
        print(f"the value of d is{self.d}and f is {self.f}and value of a is{self.x}") 
l=Deteil()   #object of the class 
print(l.__dict__)
l.variable(4,7)           
